<script>
    jQuery(function(){
   jQuery("#form-field-field_91c3e7d").keypress(function (e) {
     if (e.which != 8 && e.which != 0 && (e.which < 48 || e.which > 57)) {
     
      return false;
    }
    
    jQuery("#form-field-field_91c3e7d").attr('maxlength','12');
   });
});
</script>